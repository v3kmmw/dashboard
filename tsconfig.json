{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "node",
    "module": "ESNext",
    "outDir": "./dist",
    "jsx": "react-jsx",
    "strict": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "noEmitOnError": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*"]
}
